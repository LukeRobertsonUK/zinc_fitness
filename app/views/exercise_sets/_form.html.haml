= nested_form_for(@exercise_set) do |f|
  - if @exercise_set.errors.any?
    #error_explanation
      %h2
        = pluralize(@exercise_set.errors.count, "error")
        prohibited this exercise_set from being saved:
      %ul
        - @exercise_set.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :notes
    %br/
    = f.text_field :notes
  =render 'fields', f: f
  .actions
    = f.submit